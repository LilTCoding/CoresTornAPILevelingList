{
  "manifest_version": 3,
  "name": "TamperTorn",
  "version": "1.0",
  "description": "A userscript manager to run custom JavaScript on web pages, inspired by Violentmonkey.",
  "permissions": [
    "scripting",
    "storage",
    "tabs",
    "activeTab",
    "webNavigation"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}